{
  "name": "@sortql/sortql-cli",
  "version": "1.3.1",
  "author": "Leon Meka",
  "license": "MIT",
  "description": "A file management automation tool with SQL-like syntax",
  "scripts": {
    "dev": "npx tsx index.ts",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --i",
    "build": "node esbuild.config.cjs && npm link",
    "start": "node dist/index.cjs",
    "publish": "npm publish --access public",
    "prepare": "husky"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "adm-zip": "^0.5.12",
    "archiver": "^7.0.1",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "fluent-ffmpeg": "^2.1.2",
    "husky": "^9.0.11",
    "inquirer": "^9.2.16",
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/adm-zip": "^0.5.5",
    "@types/archiver": "^6.0.2",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/unzipper": "^0.10.9",
    "esbuild": "^0.20.2",
    "jest": "^29.7.0",
    "jest-esbuild": "^0.3.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "typescript": "^5.3.3"
  },
  "type": "module",
  "module": "index.ts",
  "main": "dist/index.cjs",
  "bin": {
    "sortql": "dist/index.cjs"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "file-management",
    "command-line-tool",
    "cli",
    "automation",
    "sql",
    "sortql"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonmeka/sortql.git"
  },
  "bugs": {
    "url": "https://github.com/leonmeka/sortql/issues"
  },
  "homepage": "https://github.com/leonmeka/sortql#readme"
}
